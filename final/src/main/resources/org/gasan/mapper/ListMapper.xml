<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.gasan.mapper.ListMapper">

	<select id="getScheduleListAll"
		resultType="org.gasan.domain.ScheduleVO">
  <![CDATA[
select * from schedule order by playdate
]]>
	</select>
	
	<select id="getScheduleListByName" resultType="org.gasan.domain.ScheduleVO">
	<![CDATA[
	select * from schedule where movieName = #{movieName} and playdate = #{date} order by playdate
	]]>
	
	
	</select>

</mapper>
